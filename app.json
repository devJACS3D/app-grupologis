{
  "expo": {
    "name": "grupologis-app",
    "slug": "grupologis-app",
    "version": "1.8.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": ["./app.plugin.js"],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff",
      "width": "100%",
      "height": "100%"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "icon": "./assets/icon-120.png",
      "buildNumber": "1.8.3",
      "bundleIdentifier": "mglio.com",
      "supportsTablet": true,

      "infoPlist": {
        "CFBundleIconName": "icon",
        "NSPhotoLibraryUsageDescription": "La aplicación necesita acceder a la biblioteca de medios para guardar los archivos descargados",
        "NSCameraUsageDescription": "Se requiere acceso a la cámara para tomar fotos",
        "NSLocationWhenInUseUsageDescription": "Se requiere acceso a la ubicación para mostrar información personalizada basada en la ubicación",
        "PHPhotoLibraryPreventAutomaticLimitedAccessAlert": true
      }
    },
    "android": {
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_MEDIA_LOCATION",
        "MEDIA_LIBRARY",
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "app.react2023.grupologis",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "00c0fd79-69a5-4fc0-9d1e-c64f5d218927"
      }
    },
    "sdkVersion": "48.0.4"
  }
}
